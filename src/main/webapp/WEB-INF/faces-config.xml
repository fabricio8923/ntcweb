<?xml version="1.0" encoding="utf-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    
    <name>primefaces_omega</name>
    
    <component>
        <component-type>org.primefaces.component.OmegaMenu</component-type>
        <component-class>org.primefaces.omega.component.menu.OmegaMenu</component-class>
    </component>
    
    <render-kit>
        <renderer>
            <component-family>org.primefaces.component</component-family>
            <renderer-type>org.primefaces.component.OmegaMenuRenderer</renderer-type>
            <renderer-class>org.primefaces.omega.component.menu.OmegaMenuRenderer</renderer-class>
        </renderer>
    </render-kit>
    <application>
        <el-resolver>org.primefaces.omega.jsfclasses.util.JsfCrudELResolver</el-resolver>
        <resource-bundle>
            <base-name>/MyBundle</base-name>
            <var>myBundle</var>
        </resource-bundle>
        <navigation-handler>org.primefaces.mobile.application.MobileNavigationHandler</navigation-handler>
        <resource-bundle>
            <base-name>/MyBundle</base-name>
            <var>myBundle</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/MyBundle</base-name>
            <var>myBundle</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/MyBundle</base-name>
            <var>myBundle</var>
        </resource-bundle>
    </application>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>welcome</from-outcome>
            <to-view-id>/welcomeJSF.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>logisticaPais</managed-bean-name>
        <managed-bean-class>org.primefaces.omega.jsfclasses.LogisticaPaisController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>logisticaPaisJpa</managed-bean-name>
        <managed-bean-class>org.primefaces.omega.sessionbean.LogisticaPaisFacade</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <converter>
        <converter-for-class>org.primefaces.omega.entidades.LogisticaPais</converter-for-class>
        <converter-class>org.primefaces.omega.jsfclasses.LogisticaPaisConverter</converter-class>
    </converter>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>logisticaPais_create</from-outcome>
            <to-view-id>/LogisticaPais/logisticaPais/New.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>logisticaPais_list</from-outcome>
            <to-view-id>/LogisticaPais/logisticaPais/List.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>logisticaPais_edit</from-outcome>
            <to-view-id>/LogisticaPais/logisticaPais/Edit.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>logisticaPais_detail</from-outcome>
            <to-view-id>/LogisticaPais/logisticaPais/Detail.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>